{
  "name": "hackathon-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:api": "jest --testPathPattern=api",
    "test:performance": "playwright test --grep @performance",
    "test:visual": "playwright test --grep @visual",
    "test:all": "npm run test && npm run test:e2e",
    "test:coverage": "jest --coverage",
    "test:regression": "playwright test --grep @regression",
    "test:edge-cases": "playwright test --grep @edge-case",
    "test:stress": "playwright test --grep @stress-test",
    "test:recipe-display": "playwright test tests/e2e/recipe-display-interaction.spec.js",
    "test:recipe-history": "playwright test tests/e2e/recipe-history-management.spec.js",
    "test:edge-stress": "playwright test tests/e2e/edge-cases-stress.spec.js",
    "test:manual-coverage": "npm run test:recipe-display && npm run test:recipe-history && npm run test:edge-stress",
    "playwright:install": "playwright install"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@babel/core": "^7.27.3",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/postcss": "^4.1.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "axe-playwright": "^2.0.1",
    "babel-jest": "^30.0.0-beta.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jsdom": "^26.1.0",
    "lighthouse": "^11.4.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.7",
    "vite": "^6.3.5"
  }
}
